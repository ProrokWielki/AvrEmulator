{
    "image": "pawelwarzecha/avr_emulator",
    "runArgs": [
        "--security-opt", // for tarpaulin
        "seccomp=unconfined" // for tarpaulin
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-extension-pack",
                "streetsidesoftware.code-spell-checker",
                "ms-python.python",
                "github.vscode-github-actions",
                "xaver.clang-format",
                "Gruntfuggly.todo-tree",
                "1YiB.rust-bundle",
                "jock.svg"
            ]
